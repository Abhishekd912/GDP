            </div>
        `);
        
        const categorySelect = modal.querySelector('#categorySelect');
        const editor = modal.querySelector('#searchTermsEditor');
        const termCount = modal.querySelector('#termCount');
        
        // Load first category
        loadCategoryTerms();
        
        function loadCategoryTerms() {
            const category = categorySelect.value;
            editor.value = searchCategories[category].join('\n');
            updateTermCount();
        }
        
        function updateTermCount() {
            const terms = editor.value.split('\n').filter(term => term.trim());
            termCount.textContent = terms.length;
        }
        
        editor.addEventListener('input', updateTermCount);
        modal.querySelector('#loadCategory').addEventListener('click', loadCategoryTerms);
        categorySelect.addEventListener('change', loadCategoryTerms);
        
        modal.querySelector('#saveTerms').addEventListener('click', () => {
            const category = categorySelect.value;
            const newTerms = editor.value.split('\n').filter(term => term.trim());
            searchCategories[category] = newTerms;
            
            // Rebuild allSearchTerms
            allSearchTerms = [];
            Object.values(searchCategories).forEach(cat => {
                allSearchTerms = allSearchTerms.concat(cat);
            });
            
            modal.remove();
            showNotification(`✓ ${category} category updated with ${newTerms.length} terms!`);
        });
        
        modal.querySelector('#cancelEdit').addEventListener('click', () => modal.remove());
    }

