    // Generate gaps HTML
    function generateGapsHTML() {
        return searchData.gaps.map((gap, index) => `
            <div style="display: flex; align-items: center; margin-bottom: 5px;">
                <input type="number" step="0.01" value="${gap}" data-index="${index}" class="gap-input" style="flex: 1; padding: 5px; border: none; border-radius: 4px; margin-right: 5px; font-size: 12px;">
                <button class="remove-gap" data-index="${index}" style="padding: 5px 8px; border: none; border-radius: 4px; background: #f44336; color: white; font-size: 10px; cursor: pointer;">×</button>
            </div>
        `).join('');
    }

    // Setup event listeners
    function setupEventListeners() {
        document.getElementById('startStopBtn').addEventListener('click', toggleSearch);
        document.getElementById('resetBtn').addEventListener('click', resetData);
        document.getElementById('editorBtn').addEventListener('click', openEditor);
        document.getElementById('statsBtn').addEventListener('click', openStats);
        document.getElementById('minimizeBtn').addEventListener('click', minimizePanel);
        document.getElementById('addGapBtn').addEventListener('click', addGap);
        document.getElementById('shuffleGapsBtn').addEventListener('click', shuffleGaps);
        document.getElementById('sessionLimit').addEventListener('change', updateSessionLimit);
        
        // Gap input listeners
        updateGapListeners();
        
        // Add hover effects
        const buttons = ['startStopBtn', 'resetBtn', 'editorBtn', 'statsBtn', 'addGapBtn', 'shuffleGapsBtn'];
        buttons.forEach(btnId => {
            const btn = document.getElementById(btnId);
            if (btn) {
                btn.addEventListener('mouseenter', () => btn.style.transform = 'scale(1.05)');
                btn.addEventListener('mouseleave', () => btn.style.transform = 'scale(1)');
            }
        });
    }

    // Update gap listeners
    function updateGapListeners() {
        document.querySelectorAll('.gap-input').forEach(input => {
            input.addEventListener('change', (e) => {
                const index = parseInt(e.target.dataset.index);
                const value = parseFloat(e.target.value);
                if (value > 0) {
                    searchData.gaps[index] = value;
                    saveSearchData(searchData);
                }
            });
        });
        
        document.querySelectorAll('.remove-gap').forEach(btn => {
            btn.addEventListener('click', (e) => {
                const index = parseInt(e.target.dataset.index);
                removeGap(index);
            });
        });
    }

    // Toggle search
    function toggleSearch() {
        const btn = document.getElementById('startStopBtn');
        if (isRunning) {
            stopSearching();
        } else {
            startSearching();
        }
    }

    // Start searching
    function startSearching() {
        isRunning = true;
        isOnBreak = false;
        shortBreakCount = 0;
        
        // Shuffle gaps when starting (anti-pattern feature)
        if (Math.random() < 0.7) { // 70% chance to shuffle
            shuffleGaps(false); // Silent shuffle
        }
        
        // Reset session if needed
        if (searchData.sessionSearches >= searchData.maxSessionSearches) {
            searchData.sessionSearches = 0;
            searchData.usedTerms = [];
        }
        
        searchData.breakStartTime = Date.now();
        saveSearchData(searchData);
        
        const btn = document.getElementById('startStopBtn');
        btn.innerHTML = '⏸️ Stop';
        btn.style.background = '#f44336';
        
        updateUI();
        scheduleNextSearch();
        showNotification('🤖 Search engine started!');
    }

    // Stop searching
    function stopSearching() {
        isRunning = false;
        isOnBreak = false;
        
        if (currentTimeout) {
            clearTimeout(currentTimeout);
            currentTimeout = null;
        }
        
        if (breakTimeout) {
            clearTimeout(breakTimeout);
            breakTimeout = null;
        }
        
        const btn = document.getElementById('startStopBtn');
        btn.innerHTML = '▶️ Start';
        btn.style.background = '#4CAF50';
        
        updateUI();
        showNotification('⏸️ Search engine stopped');
    }

    // Add gap
    function addGap() {
        const newGap = prompt('Enter gap in seconds (decimals allowed):');
        if (newGap && !isNaN(parseFloat(newGap)) && parseFloat(newGap) > 0) {
            searchData.gaps.push(parseFloat(newGap));
            saveSearchData(searchData);
            refreshGapsContainer();
            showNotification('Gap added successfully!');
        }
    }

    // Remove gap
    function removeGap(index) {
        if (searchData.gaps.length > 1) {
            searchData.gaps.splice(index, 1);
            saveSearchData(searchData);
            refreshGapsContainer();
            showNotification('Gap removed');
        } else {
            showNotification('Cannot remove last gap!');
        }
    }

    // Shuffle gaps
    function shuffleGaps(showNotif = true) {
        for (let i = searchData.gaps.length - 1; i > 0; i--) {
            const j = Math.floor(Math.random() * (i + 1));
            [searchData.gaps[i], searchData.gaps[j]] = [searchData.gaps[j], searchData.gaps[i]];
        }
        saveSearchData(searchData);
        refreshGapsContainer();
        
        if (showNotif) {
            const btn = document.getElementById('shuffleGapsBtn');
            btn.innerHTML = '✓ Shuffled';
            setTimeout(() => {
                btn.innerHTML = '🔄 Shuffle Gaps';
            }, 1500);
        }
    }

    // Refresh gaps container
    function refreshGapsContainer() {
        const container = document.getElementById('gapsContainer');
        container.innerHTML = generateGapsHTML();
        updateGapListeners();
    }

    // Update session limit
    function updateSessionLimit(e) {
        const value = parseInt(e.target.value);
        if (value > 0) {
            searchData.maxSessionSearches = value;
            saveSearchData(searchData);
            updateUI();
        }
    }

    // Reset data
    function resetData() {
        if (confirm('🔄 Reset all search data? This will clear your progress.')) {
            stopSearching();
            GM_deleteValue(CONFIG.storageKey);
            searchData = getSearchData();
            refreshGapsContainer();
            updateUI();
            showNotification('🔄 Data reset successfully!');
        }
    }

