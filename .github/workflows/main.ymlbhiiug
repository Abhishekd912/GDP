    // Advanced human-like typing simulation with slower speed and more pauses
    function simulateHumanTyping(element, text) {
        return new Promise((resolve) => {
            element.value = '';
            element.focus();
            let i = 0;
            
            function typeCharacter() {
                if (i < text.length) {
                    let charToType = text.charAt(i);
                    
                    // Random pause in the middle of typing (20% chance)
                    if (i > 2 && i < text.length - 2 && Math.random() < 0.2) {
                        setTimeout(typeCharacter, Math.random() * 1200 + 600);
                        return;
                    }
                    
                    element.value += charToType;
                    element.dispatchEvent(new Event('input', { bubbles: true }));
                    element.dispatchEvent(new Event('keyup', { bubbles: true }));
                    i++;
                    
                    // Variable typing speed with realistic pauses (slower now)
                    let delay = Math.random() * 200 + 150; // Slower typing speed
                    if (charToType === ' ') delay += Math.random() * 200; // Longer pause after spaces
                    if (i % 3 === 0) delay += Math.random() * 400; // More frequent thinking pauses
                    
                    // Occasionally longer pause (15% chance)
                    if (Math.random() < 0.15) {
                        delay += Math.random() * 800 + 400;
                    }
                    
                    setTimeout(typeCharacter, delay);
                } else {
                    resolve();
                }
            }
            
            // Start typing after initial delay
            setTimeout(typeCharacter, Math.random() * 1200 + 600);
        });
    }

    // Simulate human-like scrolling behavior (random, not on every search)
    function simulateHumanScroll() {
        return new Promise((resolve) => {
            // Random chance to scroll (30-50% chance, not pattern-based)
            const shouldScroll = Math.random() < (Math.random() * 0.2 + 0.3);
            
            if (shouldScroll) {
                // Random scroll direction (up or down)
                const scrollDirection = Math.random() < 0.5 ? 1 : -1;
                
                // Random scroll amount (100-500px)
                const scrollAmount = Math.floor(Math.random() * 400) + 100;
                
                // Smooth scroll animation
                const startPosition = window.scrollY;
                const targetPosition = startPosition + (scrollDirection * scrollAmount);
                const duration = Math.random() * 2000 + 1500; // 1.5-3.5 seconds
                const startTime = performance.now();
                
                function scrollStep(currentTime) {
                    const elapsed = currentTime - startTime;
                    const progress = Math.min(elapsed / duration, 1);
                    
                    // Easing function for smooth scrolling
                    const ease = progress < 0.5 
                        ? 4 * progress * progress * progress 
                        : 1 - Math.pow(-2 * progress + 2, 3) / 2;
                    
                    window.scrollTo(0, startPosition + (targetPosition - startPosition) * ease);
                    
                    if (progress < 1) {
                        requestAnimationFrame(scrollStep);
                    } else {
                        // Small pause after scrolling
                        setTimeout(resolve, Math.random() * 500 + 300);
                    }
                }
                
                requestAnimationFrame(scrollStep);
            } else {
                resolve();
            }
        });
    }

