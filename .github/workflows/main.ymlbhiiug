            }
            
            // Get unused search term - FIXED: Use lifetimeUsedTerms to never repeat searches
            const availableTerms = allSearchTerms.filter(term => !searchData.lifetimeUsedTerms.includes(term));
            
            // If all terms have been used, reset the lifetime used terms
            if (availableTerms.length === 0) {
                searchData.lifetimeUsedTerms = [];
                saveSearchData(searchData);
            }
            
            const baseTerm = availableTerms.length > 0 ? 
                availableTerms[Math.floor(Math.random() * availableTerms.length)] : 
                allSearchTerms[Math.floor(Math.random() * allSearchTerms.length)];
                
            const searchTerm = addHumanBehavior(baseTerm);
            
            // Clear search box and start typing
            await simulateHumanTyping(searchBox, searchTerm);
            
            // Wait and potentially interact with suggestions
            await new Promise(resolve => setTimeout(resolve, Math.random() * 1800 + 800));
            const suggestionClicked = await simulateSearchSuggestions();
            
            if (!suggestionClicked) {
                // Submit the search manually
                await new Promise(resolve => setTimeout(resolve, Math.random() * 1200 + 600));
                const searchForm = searchBox.closest('form');
                if (searchForm) {
                    searchForm.submit();
                } else {
                    searchBox.dispatchEvent(new KeyboardEvent('keydown', {
                        key: 'Enter',
                        code: 'Enter',
                        keyCode: 13,
                        bubbles: true
                    }));
                }
            }
            
            // Update search data - FIXED: Track in both session and lifetime arrays
            searchData.usedTerms.push(baseTerm);
            searchData.lifetimeUsedTerms.push(baseTerm);
            searchData.searchCount++;
            searchData.sessionSearches++;
            searchData.lastSearchTime = Date.now();
            saveSearchData(searchData);
            updateUI();
            
            console.log(`Search performed: "${searchTerm}" (${searchData.sessionSearches}/${searchData.maxSessionSearches})`);
            
            // Simulate scrolling after search (random, not on every search)
            await simulateHumanScroll();
            
            // Check if session limit reached
            if (searchData.sessionSearches >= searchData.maxSessionSearches) {
                stopSearching();
                showNotification('Session completed!');
                return;
            }
            
            // Check for break time
            checkForBreak();
            
            // Check for short break
            if (shouldTakeShortBreak()) {
                takeShortBreak();
                return;
            }
            
        } catch (error) {
            console.error('Search error:', error);
        }
        
        scheduleNextSearch();
    }

    // Schedule next search
    function scheduleNextSearch() {
        if (!isRunning || isOnBreak) return;
        
        const gap = getNextGap();
        const gapMs = gap * 1000;
        console.log(`Next search in ${gap.toFixed(2)} seconds`);
        updateNextSearchInfo(gap);
        
        currentTimeout = setTimeout(performSearch, gapMs);
    }

    // Break management
    function checkForBreak() {
        const timeSinceBreakStart = Date.now() - searchData.breakStartTime;
        const breakIntervalMs = CONFIG.breakInterval * 60 * 1000;
        
        if (timeSinceBreakStart >= breakIntervalMs) {
            takeBreak();
        }
    }

    function takeBreak() {
        isOnBreak = true;
        const breakDuration = (CONFIG.microBreakDuration[0] + Math.random() * (CONFIG.microBreakDuration[1] - CONFIG.microBreakDuration[0])) * 60 * 1000;
        
        showNotification(`Taking a ${Math.round(breakDuration/60000)} minute break...`);
        updateUI();
        
        breakTimeout = setTimeout(() => {
            isOnBreak = false;
            searchData.breakStartTime = Date.now();
            saveSearchData(searchData);
            updateUI();
            
            if (isRunning) {
                scheduleNextSearch();
            }
        }, breakDuration);
    }

